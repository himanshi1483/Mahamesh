
@{
    ViewBag.Title = "Ministers";
}


<style>
    #mu-footer {
        display: inline;
        float: left;
        width: 100%;
        position: fixed;
        left: 0;
        bottom: 0;
        /*padding-top: 30px;*/
    }
</style>
<div class="container body-content" id="#maincontent" style="margin-top:20px; margin-bottom: 150px;">
    <div class="panel panel-primary-2" style="box-shadow: 0 2px 2px rgba(0,0,0,.3);">
        <div class="panel-heading panel-primary-2">
            <h3 class="panel-title">@Resource.Lang.Ministers</h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <table class="table table-bordered table-responsive table-striped" style="font-size:13px">
                    <thead>
                        <tr>
                            <th>
                                @Resource.Lang.SrNo
                            </th>
                            <th class="text-center">@Resource.Lang.MInistersName</th>
                            <th class="text-center">
                                @Resource.Lang.MinisterAdd
                            </th>
                            <th class="text-center">@Resource.Lang.PhoneNumber</th>
                            <th class="text-center">@Resource.Lang.EmailID</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="text-center">1.</td>
                            <td>
                                @Html.Raw(Server.HtmlDecode(Resource.Lang.Minister1Name))
                            </td>
                            <td>   @Html.Raw(Server.HtmlDecode(Resource.Lang.Minister1Address))</td>
                            <td>@Resource.Lang.Minister1Phone </td>
                            <td>
                                min.adf1@maharashtra.gov.in
                            </td>
                        </tr>
                        <tr>
                            <td class="text-center">2.</td>
                            <td>
                                @Html.Raw(Server.HtmlDecode(Resource.Lang.Minister2Name))
                            </td>
                            <td>  @Html.Raw(Server.HtmlDecode(Resource.Lang.Minister1Address))</td>
                            <td>@Resource.Lang.Minister2Phone</td>
                            <td>
                                stmin.adfntextiles@maharashtra.gov.in
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="/Content/Varsity/assets/js/jquery.min.js"></script>

<script type="text/javascript">
    $(document).ready(function () {
        $(".nav li").removeClass("active");//this will remove the active class from
        //previously active menu item
        $('#org').addClass('active');
        //for demo
        //$('#demo').addClass('active');
        //for sale
        //$('#sale').addClass('active');
    });
</script>