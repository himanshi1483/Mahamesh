
@{
    ViewBag.Title = "Pohara";
}

<style>
    .div-header {
        text-align: center;
        font-size: 32px;
        color: #fff;
        font-weight: 700;
        /*height:40px;*/
        background: #65bff3;
        font-variant-caps: all-petite-caps;
    }
</style>
<div class="container  body-content" id="#maincontent" style="margin-bottom:10%">
    <div class="div-header">@Resource.Lang.Pohara</div>
    <section>
        <div class="panel panel-info">
            <div class="heading text-center"><h4>@Resource.Lang.FarmInformation</h4></div>

            <div class="panel-body">
                <table class="table table-bordered table-responsive table-striped table-hover">
                    <thead>
                        <tr>
                            <th class="text-center">@Resource.Lang.Description</th>
                            <th class="text-center">@Resource.Lang.Details</th>
                        </tr>
                    </thead>
                    <tbody class="text-center">
                        <tr>
                            <td>@Resource.Lang.NameofFarm</td>
                            <td>@Resource.Lang.PoharaNameofFarm</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.FarmAddress</td>
                            <td>@Resource.Lang.PoharaFarmAddress</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.PhoneNumber</td>
                            <td>@Resource.Lang.PoharaPhoneNumber</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.EmailID</td>
                            <td>
                                sgf.pohara@gmail.com
                            </td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.NameofFarmManager</td>
                            <td>@Resource.Lang.PoharaNameofFarmManager</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.MobileNumber</td>
                            <td>@Resource.Lang.PoharaTotalLand</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.TotalLand</td>
                            <td>@Resource.Lang.PoharaTotalLand</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.IrrigationLand</td>
                            <td>@Resource.Lang.PoharaIrrigationLand</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.NonIrrigationLand</td>
                            <td>@Resource.Lang.PoharaNonIrrigationLand</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.LandUnderBuilding</td>
                            <td>@Resource.Lang.PoharaLandUnderBuilding</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.AreaunderGrazing</td>
                            <td>@Resource.Lang.PoharaAreaunderGrazingPasture</td>
                        </tr>

                    </tbody>
                </table>
                <hr />
                <div class="heading">
                    <h4 class="text-center">@Resource.Lang.LiveStockDetails</h4>
                </div>
                <table class="table table-bordered table-responsive table-striped table-hover text-center">
                    <thead>
                        <tr>
                            <th class="text-center">@Resource.Lang.Breed</th>
                            <th class="text-center">@Resource.Lang.Male</th>
                            <th class="text-center">@Resource.Lang.Female</th>
                            <th class="text-center">@Resource.Lang.MaleKids</th>
                            <th class="text-center">@Resource.Lang.FemaleKids</th>
                            <th class="text-center">@Resource.Lang.Total</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>@Resource.Lang.OsmanabadiGoat</td>
                            <td>15</td>
                            <td>138</td>
                            <td>29</td>
                            <td>24</td>
                            <td>206</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.BerariGoat</td>
                            <td>5</td>
                            <td>55</td>
                            <td>13</td>
                            <td>27</td>
                            <td>100</td>
                        </tr>
                    </tbody>
                </table>
                <hr />
                <div class="heading">
                    <h4 class="text-center">@Resource.Lang.FacilitiesAvailable</h4>
                </div>
                <table class="table table-bordered table-responsive table-striped table-hover text-center">
                    <thead>
                        <tr>
                            <th class="text-center">@Resource.Lang.Facility</th>
                            <th class="text-center">@Resource.Lang.Description</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>@Resource.Lang.SupplyofBucks</td>
                            <td class="text-left">@Resource.Lang.AmbajogaiSupplyDetail</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.SheepGoatFarmingTraining</td>
                            <td class="text-left">
                                @Resource.Lang.BilakhedTraining
                            </td>
                        </tr>

                        <tr>
                            <td>@Resource.Lang.PurchaseofWool</td>
                            <td class="text-left">@Resource.Lang.AmbajogaiPurchase</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.ProductionSupply</td>
                            <td class="text-left">@Resource.Lang.AmbajogaiProduction</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.ExtensionActivity</td>
                            <td class="text-left">@Resource.Lang.AmbajogaiExtensionActivity</td>
                        </tr>
                        <tr>
                            <td>@Resource.Lang.SheepGoatUnitDistribution</td>
                            <td class="text-left">@Resource.Lang.AmbajogaiDistribution</td>
                        </tr>
                       

                    </tbody>
                </table>
            </div>
        </div>
    </section>
</div>

